{
  "name": "instatdlr-ts-services",
  "version": "1.0.0",
  "description": "TypeScript services for InstaTLDR",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist",
    "start": "bun run dist/index.js",
    "dev": "bun run --watch src/index.ts",
    "score": "bun run src/scripts/runScorer.ts",
    "test:scorer": "bun run src/scripts/testScorer.ts"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "bun-types": "latest",
    "typescript": "^5.0.4"
  }
}